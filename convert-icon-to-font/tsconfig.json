{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "baseUrl": "src",
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@figma"
    ],
    "lib": [
      "es6",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/index.ts",
    "manifest.json"
  ]
}
